parameters:
    level: 6
    treatPhpDocTypesAsCertain: false
    ignoreErrors:
        # To support psr-11 without set method
        - '#Call to an undefined method Psr\\Container\\ContainerInterface::set\(\)#'
        # RelatedProxyTrait is not really used
        - '#Trait Blrf\\Orm\\Model\\RelatedProxyTrait is used zero times and is not analysed#'

    paths:
        - src/

    fileExtensions:
        - php